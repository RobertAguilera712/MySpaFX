<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ReservacionesFormController">
   <children>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308">
         <children>
            <Label styleClass="h1" text="Agregar reservación" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308" spacing="20.0">
         <children>
            <JFXDatePicker prefWidth="215.0" promptText="Fecha de reservación" />
            <JFXComboBox promptText="Selecciona un numero de cliente" styleClass="input" />
            <JFXComboBox layoutX="141.0" layoutY="10.0" promptText="Selecciona una sala" styleClass="input" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="82.0" maxHeight="1.7976931348623157E308" spacing="20.0">
         <children>
            <JFXButton text="Guardar">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-primary" />
               </styleClass>
            </JFXButton>
            <JFXButton layoutX="333.0" layoutY="10.0" onAction="#regresar" text="Regresar">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-danger" />
               </styleClass>
            </JFXButton>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@styles/icons.css" />
      <URL value="@styles/styles.css" />
   </stylesheets>
</VBox>
